{
  "name": "pstree",
  "shortDescription": "display a tree of processes",
  "longDescription": "pstree shows running processes as a tree.  The tree is rooted at either pid or init if pid is omitted.   If  a user name is specified, all process trees rooted at processes owned by that user are shown.",
  "syntaxes": [
    "pstree [-a, --arguments] [-c, --compact] [-h, --highlight-all, -Hpid, --highlight-pid pid] [-g] --show-pgids]",
    "[-l, --long] [-n, --numeric-sort] [-N, --ns-sortns [-p, --show-pids] [-s, --show-parents] [-S, --ns-changes]",
    "[-t, --thread-names] [-T, --hide-threads] [-u, --uid-changes] [-Z, --security-context]",
    "[-A, --ascii, -G, --vt100, -U, --unicode] [pid, user]",
    "pstree -V, --version"
  ],
  "options": [
    {
      "name": "arguments",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "a",
      "isLong": true
    },
    {
      "name": "ascii",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "A",
      "isLong": true
    },
    {
      "name": "compact",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "c",
      "isLong": true
    },
    {
      "name": "vt100",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "G",
      "isLong": true
    },
    {
      "name": "highlight-all",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "h",
      "isLong": true
    },
    {
      "name": "highlight-pid",
      "argumentType": "ARGUMENT_REQUIRED",
      "equivalent": "H",
      "isLong": true
    },
    {
      "name": "long",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "l",
      "isLong": true
    },
    {
      "name": "numeric-sort",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "n",
      "isLong": true
    },
    {
      "name": "ns-sort",
      "argumentType": "ARGUMENT_REQUIRED",
      "equivalent": "N",
      "isLong": true
    },
    {
      "name": "show-pids",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "p",
      "isLong": true
    },
    {
      "name": "show-pgids",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "g",
      "isLong": true
    },
    {
      "name": "show-parents",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "s",
      "isLong": true
    },
    {
      "name": "ns-changes",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "S",
      "isLong": true
    },
    {
      "name": "thread-names",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "t",
      "isLong": true
    },
    {
      "name": "hide-threads",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "T",
      "isLong": true
    },
    {
      "name": "uid-changes",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "u",
      "isLong": true
    },
    {
      "name": "unicode",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "U",
      "isLong": true
    },
    {
      "name": "version",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "V",
      "isLong": true
    },
    {
      "name": "security-context",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "Z",
      "isLong": true
    }
  ]
}