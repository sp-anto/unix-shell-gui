{
  "name": "pstree",
  "shortDescription": "display a tree of processes",
  "longDescription": "pstree shows running processes as a tree.  The tree is rooted at either pid or init if pid is omitted.   If  a user name is specified, all process trees rooted at processes owned by that user are shown.",
  "syntaxes": [
    "pstree [OPTION]... PID",
    "pstree [OPTION]... USER",
    "pstree --version"
  ],
  "options": [
    {
      "name": "arguments",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "a",
      "description": "Show command line arguments.",
      "isLong": true
    },
    {
      "name": "ascii",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "A",
      "description": "Use ASCII line drawing characters.",
      "isLong": true
    },
    {
      "name": "compact",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "c",
      "description": "Don't compact identical subtrees.",
      "isLong": true
    },
    {
      "name": "vt100",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "G",
      "description": "Use VT100 line drawing characters.",
      "isLong": true
    },
    {
      "name": "highlight-all",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "h",
      "description": "Highlight current process and its ancestors.",
      "isLong": true
    },
    {
      "name": "highlight-pid",
      "argumentType": "ARGUMENT_REQUIRED",
      "equivalent": "H",
      "description": "Highlight this process and its ancestors.",
      "isLong": true
    },
    {
      "name": "long",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "l",
      "description": "Don't truncate long lines.",
      "isLong": true
    },
    {
      "name": "numeric-sort",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "n",
      "description": "Sort output by PID.",
      "isLong": true
    },
    {
      "name": "ns-sort",
      "argumentType": "ARGUMENT_REQUIRED",
      "equivalent": "N",
      "description": "Sort by namespace type (cgroup, ipc, mnt, net, pid, user, uts).",
      "isLong": true
    },
    {
      "name": "show-pids",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "p",
      "description": "Show PIDs; implies -c.",
      "isLong": true
    },
    {
      "name": "show-pgids",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "g",
      "description": "Show process group ids; implies -c.",
      "isLong": true
    },
    {
      "name": "show-parents",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "s",
      "description": "Show parents of the selected process.",
      "isLong": true
    },
    {
      "name": "ns-changes",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "S",
      "description": "Show namespace transitions.",
      "isLong": true
    },
    {
      "name": "thread-names",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "t",
      "description": "Show full thread names.",
      "isLong": true
    },
    {
      "name": "hide-threads",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "T",
      "description": "Hide threads, show only processes.",
      "isLong": true
    },
    {
      "name": "uid-changes",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "u",
      "description": "Show uid transitions.",
      "isLong": true
    },
    {
      "name": "unicode",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "U",
      "description": "Use UTF-8 (Unicode) line drawing characters.",
      "isLong": true
    },
    {
      "name": "version",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "V",
      "description": "Display version information.",
      "isLong": true
    },
    {
      "name": "security-context",
      "argumentType": "NO_ARGUMENT",
      "equivalent": "Z",
      "description": "Show SELinux security contexts.",
      "isLong": true
    }
  ]
}
